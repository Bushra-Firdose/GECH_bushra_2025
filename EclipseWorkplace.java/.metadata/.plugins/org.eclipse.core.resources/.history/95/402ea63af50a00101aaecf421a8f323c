package PredfinedFunctionInterface;

import java.util.Arrays;
import java.util.List;
import java.util.stream.Stream;

public class StramsInJava1 {

	public static void main(String[] args) {
		List<Integer> list = Arrays.asList(12,13,14,15,23,45);
		Stream<Integer> streams = list.stream().fiter(x->x>10);
		Stream<Integer> stream;
		Long res = stream.count();
		System.out.println(res);
		List<Integer> result = stream.collect(Collectrs.toList());
		System.out.println(result);
		
		Long res1=streams.count();
		System.out.println(res1);
		

	}

}
