package abstraction;

import java.security.PublicKey;

//1.Interface
interface Animal{
	public void  eat();
}
class Lion implements Animal{
	
	@Override
	public void eat() {
		System.out.println("Lion will eat meat");
	}
	
}

//2.abstract class
abstract class Vehicle{
	//abm
	public abstract void satrt();


	//non-abm
	public void stop() {
		System.out.println("Lion will eat vegitables");
	}
	abstract class Car extends Vehicle{

		@Override
		public void start() {
			System.out.println("The car Starts");
			
		}
		
	}




public class Abstraction {
public static void main(String[] args) {
	/*
	 * Abstraction
	 * it is a process of binding the implementation and 
	 * showing only the essention resource
	 * 
	 * 2 ways :
	 * 1.Interface (100%)
	 * abstract class(0-100%)*/
}
}
